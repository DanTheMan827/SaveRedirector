{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.shared.schema.json",
  "config": {
    "version": "0.4.0",
    "sharedDir": "shared",
    "dependenciesDir": "extern",
    "info": {
      "name": "SaveRedirector",
      "id": "SaveRedirector",
      "version": "0.1.0",
      "url": null,
      "additionalData": {
        "overrideSoName": "libSaveRedirector.so",
        "cmake": true
      }
    },
    "workspace": {
      "scripts": {
        "build": [
          "pwsh ./scripts/build.ps1"
        ],
        "clean": [
          "pwsh ./scripts/build.ps1 -clean"
        ],
        "clean-qmod": [
          "qpm s clean",
          "qpm s qmod"
        ],
        "copy": [
          "pwsh ./scripts/copy.ps1 $0:?"
        ],
        "deepclean": [
          "pwsh ./scripts/build.ps1 -clean -skipBuild",
          "qpm clear"
        ],
        "logcat": [
          "pwsh ./scripts/start-logging.ps1"
        ],
        "make-hooking": [
          "pwsh ./common-stuff/scripts/make-hooking.ps1"
        ],
        "qmod": [
          "qpm s build",
          "qpm s update-version",
          "qpm qmod zip --skip_build",
          "qpm s update-version -- -restore"
        ],
        "rebuild": [
          "qpm s deepclean",
          "qpm s build"
        ],
        "rebuild-qmod": [
          "qpm s deepclean",
          "qpm s qmod"
        ],
        "restart": [
          "pwsh ./scripts/restart-game.ps1"
        ],
        "stack": [
          "pwsh ./scripts/ndk-stack.ps1"
        ],
        "tomb": [
          "pwsh ./scripts/pull-tombstone.ps1"
        ],
        "update-version": [
          "pwsh ./scripts/update-version.ps1 $0:?"
        ]
      },
      "ndk": "^27.2.12479018",
      "qmodIncludeDirs": [
        "./build",
        "./extern/libs"
      ],
      "qmodIncludeFiles": [],
      "qmodOutput": "./SaveRedirector.qmod"
    },
    "dependencies": [
      {
        "id": "And64InlineHook",
        "versionRange": "^0.1.0",
        "additionalData": {}
      },
      {
        "id": "audica-hook",
        "versionRange": "^0.1.3",
        "additionalData": {}
      },
      {
        "id": "paper2_scotland2",
        "versionRange": "^4.6.4",
        "additionalData": {}
      },
      {
        "id": "scotland2",
        "versionRange": "^0.1.6",
        "additionalData": {
          "includeQmod": false
        }
      }
    ]
  },
  "restoredDependencies": [
    {
      "dependency": {
        "id": "audica-hook",
        "versionRange": "=0.1.3",
